{
  "require": {
    "craftcms/cms": "3.7.34",
    "craftcms/redactor": "2.10.5",
    "dolphiq/redirect": "1.1.1",
    "mmikkel/reasons": "2.2.6",
    "mmikkel/retcon": "2.3.0",
    "mustasj/expanded-search": "1.3.7",
    "nystudio107/craft-fastcgicachebust": "^1.0.9",
    "nystudio107/craft-minify": "1.2.11",
    "nystudio107/craft-seomatic": "3.4.26",
    "nystudio107/craft-twigpack": "1.2.15",
    "nystudio107/craft-typogrify": "1.1.19",
    "putyourlightson/craft-sherlock": "3.1.4",
    "putyourlightson/craft-sprig": "1.11.1",
    "sebastianlenz/linkfield": "1.0.25",
    "solspace/craft-express-forms": "1.1.2",
    "studioespresso/craft-navigate": "2.8.1",
    "topshelfcraft/environment-label": "3.2.0",
    "vaersaagod/dospaces": "^1.1.3",
    "vaersaagod/matrixmate": "1.3.4",
    "verbb/knock-knock": "1.2.16",
    "verbb/super-table": "2.7.1",
    "vlucas/phpdotenv": "^3.4.0",
    "yiisoft/yii2-redis": "^2.0.6"
  },
  "require-dev": {
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "autoload": {
    "psr-4": {
      "modules\\sitemodule\\": "modules/sitemodule/src/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.3"
    }
  },
  "scripts": {
    "craft-update": [
      "@php craft migrate/all",
      "@php craft project-config/apply",
      "@php craft clear-caches/all",
      "chmod 0777 .env",
      "chmod 0777 composer.json",
      "chmod 0777 composer.lock",
      "chmod 0777 config",
      "chmod -R 0777 config/project",
      "chmod -f 0777 config/license.key || true",      
      "chmod -R 0777 storage",
      "chmod -R 0777 vendor",
      "chmod -R 0777 web/cpresources"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('example.env', '.env');\""
    ],
    "post-update-cmd": "@craft-update",
    "post-install-cmd": "@craft-update"
  }
}

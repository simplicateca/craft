/**
 * _settings/utility.css
 * 
 * Utility classes
 *
 */

.more {
    &:after {
        content: '';
        position: absolute;
        top: 0;
        right: 0;
        bottom: 0;
        left: 0;
        z-index: 2;
    }
}

a {
    @apply text-blue-600 underline;

    &:hover, &:focus {
        @apply text-blue-900;
    }
}